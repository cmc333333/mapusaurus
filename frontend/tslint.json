{
  "extends": ["tslint-config-airbnb", "tslint:latest", "tslint-react"],
  "rules": {
    "array-type": [true, "array"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "function-name": false,
    "import-name": [true, {"reactMapGl": "ReactMapGL" }],
    "interface-name": [true, "never-prefix"],
    "no-implicit-dependencies": [true, "dev"],
    "no-shadowed-variable": false,
    "no-submodule-imports": [true, "mapbox-gl/dist/mapbox-gl.css", "normalize.css/normalize.css"],
    "ter-arrow-parens": [true, "as-needed"]
  }
}
